{
    "gpu": {
        "use": true,
        "id": 1
    },
    
    "model": "GIN",
    "dataset": "TSP",
    
    "out_dir": "out/TSP_edge_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 500,
        "batch_size": 64,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 1,
        "max_time": 48
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 80,
        "residual": false,
        "readout": "sum",
        "n_mlp_GIN": 2,
        "learn_eps_GIN": true,
        "neighbor_aggr_GIN": "sum",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "graph_norm": true,
        "batch_norm": true
    }
}